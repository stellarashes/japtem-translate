{
	"name": "Japtem Translate",
	"description": "Chrome app to facilitate translation process for novels on Japtem",
	"version": "0.1",
	"manifest_version": 2,
	"app": {
		"background": {
			"page": "background.html"
		}
	},
	"icons": {
		"16": "JTH-16.png",
		"128": "JTH-128.png"
	},
	"permissions":[
		"tts",
		"storage",
		{
			"fileSystem": ["write", "retainEntries"]
		},
		"clipboardWrite",
		"alwaysOnTopWindows"
	]
}